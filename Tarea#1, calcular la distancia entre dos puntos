#include <iostream>
#include "math.h"

using namespace std;

int main()
{
    float X1, X2, Y1, Y2, d;
    cout<<"Ingrese las coordenadas en 'X' y 'Y' del primer punto"<<endl<<"X1: ";
    cin>>X1;
    cout<<"Y1: ";
    cin>>Y1;
    cout<<"Ingrese las coordenadas en 'X' y 'Y' del segundo punto"<<endl<<"X2: ";
    cin>>X2;
    cout<<"Y2: ";
    cin>>Y2;
    d=sqrt((X2-X1)*(X2-X1)+(Y2-Y1)*(Y2-Y1));
    cout<<"La distancia entre los dos puntos es de: "<<d;

    return 0;
}
